      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "postcss-cli": "^7.1.1",
    "pts": "^0.9.4",
    "react-scroll": "^1.7.16",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.4.6"
  }
